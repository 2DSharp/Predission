{
  "name": "twodee/predission",
  "type": "library",
  "homepage": "https://github.com/2dsharp/predission",
  "description": "Redis based session management library",

  "authors": [
    {
      "name": "Dedipyaman Das",
      "email": "2d@twodee.me",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Predission\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Predission\\UnitTest\\" : "tests/UnitTests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "predis/predis": "^1.1",
    "ircmaxell/random-lib": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.1"
  },

  "config": {
    "platform": {
      "php": "7.2.9"
    },
    "platform-dev": {
      "php": "7.2.9"
    }
  }
}
